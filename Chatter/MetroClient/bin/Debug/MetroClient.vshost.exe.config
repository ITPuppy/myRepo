<?xml version="1.0"?>
<configuration>
    <startup> 
        
    <supportedRuntime version="v2.0.50727"/></startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
              <binding name="myBinding" sendTimeout="00:00:5">
                <reliableSession enabled="true" inactivityTimeout="0:1:00"/>
                <security mode="None"/>
              </binding>
                <binding name="NetTcpBinding_IChatter" sendTimeout="00:01:10" >
                    <reliableSession inactivityTimeout="00:01:00" enabled="true" />
                    <security mode="None" />
                </binding>
                <binding name="NetTcpBinding_IRegister">
                    <reliableSession inactivityTimeout="00:01:00" enabled="true" />
                    <security mode="None" />
                </binding>
            </netTcpBinding>
          <netPeerTcpBinding>
            <binding name="PeerTcpConfig" port="0">
              <security mode="None"></security>
              <resolver mode="Custom"  >
                <custom address="net.tcp://222.26.186.164:9877/Resolver" binding="netTcpBinding" bindingConfiguration="myBinding" ></custom>
              </resolver>
              
            </binding>
          </netPeerTcpBinding>
         
        </bindings>
        <client>
            <endpoint address="net.tcp://222.26.186.164:9877/RegisterService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IRegister"
                contract="RegisterService.IRegister" name="NetTcpBinding_IRegister" />
            <endpoint address="net.tcp://222.26.186.164:9877/ChatterService" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IChatter" contract="ChatterService.IChatter"
                name="NetTcpBinding_IChatter" />

          <endpoint  name="p2p"
                     binding="netPeerTcpBinding" 
                     bindingConfiguration="PeerTcpConfig" 
                     contract="Chatter.MetroClient.P2P.IP2PChatService"
                     ></endpoint>
        </client>
    </system.serviceModel>

<appSettings> 
    <add key="IsSupportGroup" value="no"/> 
  </appSettings> 
</configuration>
